{
  "name": "ky-drywall-&-steel-frame",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:prerender": "vite build && react-snap",
    "preview": "vite preview"
  },
  "reactSnap": {
    "source": "dist",
    "include": [
      "/",
      "/empresa",
      "/contato",
      "/produtos",
      "/servicos",
      "/blog",
      "/sitemap"
    ],
    "minifyHtml": {
      "collapseWhitespace": false,
      "removeComments": false
    },
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ],
    "skipThirdPartyRequests": true
  },
  "dependencies": {
    "lucide-react": "^0.475.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^7.11.0",
    "react-snap": "^1.23.0"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
}
